   "source": [
    "s = 8\n",
    "A = np.zeros((s, s), dtype=np.int32)\n",
    "\n",
    "for i in range(s):\n",
    "    for j in range(s):\n",
    "        if i % 2 == 0:\n",
    "            A[i][j] = i + j\n",
    "        else:\n",
    "            A[i][j] = i * j\n",
    "        \n",
    "A"
   ]
